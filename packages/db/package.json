{
  "name": "@agentic-mm-bot/db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./get-db": "./src/get-db.ts"
  },
  "scripts": {
    "typecheck": "tsgo --noEmit",
    "clean": "rm -rf dist",
    "db:generate": "drizzle-kit generate",
    "db:push": "dotenvx run -f ../../.encrypted.local -fk ../../.env.keys -- drizzle-kit push",
    "db:studio": "dotenvx run -f ../../.encrypted.local -fk ../../.env.keys -- drizzle-kit studio",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^3.0.4",
    "drizzle-orm": "catalog:",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@agentic-mm-bot/typescript-config": "workspace:*",
    "@posaune0423/eslint-config": "catalog:tooling",
    "@types/node": "catalog:tooling",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "catalog:tooling",
    "typescript": "catalog:tooling"
  }
}
