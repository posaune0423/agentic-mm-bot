{
  "name": "agentic-mm-bot",
  "private": true,
  "packageManager": "bun@1.3.6",
  "engines": {
    "node": ">=24.0.0"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "zod": "latest",
      "drizzle-orm": "latest"
    },
    "catalogs": {
      "tooling": {
        "@dotenvx/dotenvx": "latest",
        "@prettier/plugin-oxc": "latest",
        "@types/node": "latest",
        "@types/bun": "latest",
        "@typescript/native-preview": "latest",
        "eslint": "latest",
        "prettier": "latest",
        "prettier-plugin-organize-imports": "latest",
        "prettier-plugin-tailwindcss": "latest",
        "typescript": "latest",
        "@t3-oss/env-core": "latest"
      }
    }
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "postinstall": "bun run scripts/ensure-extended-sdk-wasm.ts",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier --check . --experimental-cli --experimental-ternaries",
    "format:fix": "prettier --write . --experimental-cli --experimental-ternaries",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "db:studio": "turbo run db:studio",
    "clean": "bun scripts/clean.ts",
    "cancel:all-orders": "dotenvx run -f .encrypted.local -fk .env.keys -- bun run scripts/cancel-all-orders.ts",
    "migrate:llm-proposals-v2": "dotenvx run -f .encrypted.local -fk .env.keys -- bun run scripts/migrate-llm-proposals-v2.ts"
  },
  "devDependencies": {
    "@agentic-mm-bot/adapters": "workspace:*",
    "@agentic-mm-bot/db": "workspace:*",
    "@agentic-mm-bot/prettier-config": "workspace:*",
    "@dotenvx/dotenvx": "catalog:tooling",
    "@prettier/plugin-oxc": "^0.1.3",
    "@types/bun": "^1.3.6",
    "@types/node": "^25.0.8",
    "@typescript/native-preview": "^7.0.0-dev.20260114.1",
    "drizzle-orm": "^0.45.1",
    "jiti": "^2.6.1",
    "prettier": "^3.8.0",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "turbo": "^2.7.4",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "typescript": "catalog:tooling"
  },
  "prettier": "@agentic-mm-bot/prettier-config"
}
