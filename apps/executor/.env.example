# Copy this file to `.env` and set real values.
DATABASE_URL="postgres://dev:insecure@localhost:5432/main?sslmode=disable"

# Logging
LOG_LEVEL=DEBUG
LOG_DIR=./logs

# App
APP_ENV=development

# Trading
EXCHANGE=extended
SYMBOL=BTC-USD

# Extended Exchange
EXTENDED_NETWORK=mainnet
EXTENDED_API_KEY=YOUR_EXTENDED_API_KEY
EXTENDED_STARK_PRIVATE_KEY=YOUR_EXTENDED_STARK_PRIVATE_KEY
EXTENDED_STARK_PUBLIC_KEY=YOUR_EXTENDED_STARK_PUBLIC_KEY
