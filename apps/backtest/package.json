{
  "name": "@agentic-mm-bot/backtest",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun -e \"console.log('Backtest dev: skipped (use: bun run start).'); setInterval(() => {}, 1 << 30)\"",
    "start": "dotenvx run -f ../../.encrypted.local -fk ../../.env.keys -- bun run src/main.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsgo --noEmit",
    "test": "dotenvx run -f ../../.encrypted.local -fk ../../.env.keys -- bun test"
  },
  "dependencies": {
    "@agentic-mm-bot/core": "workspace:*",
    "@agentic-mm-bot/db": "workspace:*",
    "@agentic-mm-bot/repositories": "workspace:*",
    "@agentic-mm-bot/utils": "workspace:*",
    "@t3-oss/env-core": "catalog:tooling",
    "drizzle-orm": "catalog:",
    "neverthrow": "^8.2.0",
    "pg": "^8.16.3",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@agentic-mm-bot/typescript-config": "workspace:*",
    "@posaune0423/eslint-config": "catalog:tooling",
    "@types/bun": "catalog:tooling",
    "@types/pg": "^8.16.0",
    "@typescript/native-preview": "catalog:tooling",
    "eslint": "catalog:tooling",
    "typescript": "catalog:tooling"
  }
}
